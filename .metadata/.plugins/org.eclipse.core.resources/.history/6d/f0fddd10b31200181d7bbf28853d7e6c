package a3;

import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;

public class VideoGame {
	private static class Edge{
		public int start, end, change;
		Edge(int u, int v, int c) {
			start = u;
			end = v; 
			change = c;
		}
	}
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		for (int t = 0; t < T; t++) {
			int N, M, K;
			N = sc.nextInt();
			M = sc.nextInt();
			K = sc.nextInt();
			int[] dif = new int[N];
			dif[0] = K;
			for (int i = 1; i < N; i++)
				dif[i] = Integer.MIN_VALUE;
			List<Edge> edges = new ArrayList<>();
			for (int m = 0; m < M; m++)
				edges.add(new Edge(sc.nextInt()-1, sc.nextInt()-1, sc.nextInt())); // -1 because counting starts at 0, dammit
			// OK, have everything in memory now. Do Bellman-Ford.
			for (int n = 0; n < N-1; n++)
				for (Edge e : edges)
		           if (dif[e.start] != Integer.MIN_VALUE && dif[e.end] + e.change > dif[e.start]) {
		        	   dif[e.end] = dif[e.start] + e.change;
		        	   System.out.println("ey "+e.start+" -"+e.change+">"+e.end);
		           }
			if (dif[N-1] == Integer.MIN_VALUE)
				System.out.println("-1");
			else {
				boolean relax = false;
				for (int n = 0; n < N; n++)
					for (Edge e : edges)
			           if (dif[e.end] + e.change > dif[e.start])
			        	   relax = true;
				if (relax)
					System.out.println("Infinity");
				else
					System.out.println(dif[N-1]);
			}
		}
		sc.close();
	}
}
